import java.util.HashMap;


 class GUI {
	 
HashMap<String, Integer> progress = new HashMap<String, Integer>(); 	 

public void addFileOnGUI(String FileName, int totalNumberofPeices ){
	progress.put(FileName, totalNumberofPeices);
	}	 
	 
	 
public inform(int numberofPeices){
	int progress=numberofPeices/totalNumberofPeices;
	
}	 

}
