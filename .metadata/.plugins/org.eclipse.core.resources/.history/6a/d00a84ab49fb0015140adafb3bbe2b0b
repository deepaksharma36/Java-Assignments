import java.io.File;
import java.io.IOException;
import java.util.ArrayList;


public class Profiler {

	
	public static void main(String[] args) {
		TestCaseGenerator aTestCaseGenerator = new TestCaseGenerator();
		TestCaseExecutor aTestCaseExecutor = new TestCaseExecutor("FirstAlgo","testRecords");
		ArrayList<String> testType=new ArrayList<String>();
		testType.add("DNA");
		testType.add("alphabets");
		try {
			File testCaseFile =aTestCaseGenerator.TestCasesGenerator(50,testType ,"test.txt");
			aTestCaseExecutor.TestExecutor(testCaseFile);
			
		} catch (IOException e) {
	
		}
		
	}

}
