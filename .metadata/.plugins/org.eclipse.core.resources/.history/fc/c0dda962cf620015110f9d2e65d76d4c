/*
 * Test.java
 * 
 * Doubts regarding array of object creation
 */
class Test {
	int x = 1;
}
public class Test {
	public static void main(String[] args) {
		int array_int[]; 
		array_int = new int[5]; 
		Test cl_a = new Test(); // reference to a object
		System.out.println("1 " + cl_a);
		Test[] cl_array;// reference to the array
		cl_array = new Test[5]; 
		// cl_array[1]= new A();
		System.out.println("2 " + cl_array);
		System.out.println("3 " + cl_array[1]);
		try {
			System.out.println("4 " + cl_array[1].x);
		} catch (Exception e) {
			System.out.println("5 " + e);

		}
	}

}
